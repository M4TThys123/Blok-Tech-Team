<a href="filter"><img src="/public/images/filter.svg"> Filter</a>
<section class="match">
    {{#each profielen}}
    <article>
        <img id="profileimage" src="{{this.afbeelding}}">
        <h2>{{this.naam}}</h2>
        <h4>{{this.leeftijd}}</h4>
        <p>{{this.beschrijving}}</p>
        <section id="games">
            {{!-- <h2>Favorieten games</h2> --}}
            <ul>
                {{#each favoritegames}}
                <li>
                    <img src="/public/images/{{this}}.jpg" alt={{this}}>
                </li>
                {{/each}}
            </ul>
        </section>
        <div class='buttons'>
            <img src="/public/images/close.svg">
            <img src="/public/images/heart.svg">
        </div>
    </article>
    {{/each}}
</section>